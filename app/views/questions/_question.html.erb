<% div_for question do %>
  <%= voting_booth question %>
	<%= link_to question.name, question %>
	<%= link_to question.comments.count, question %>
	<%= tagger question %>
	<%= tag_display question %>
	<%= add_to_list question %>
	<% if current_user.andand.admin? %>
	  [<%= link_to 'x', question_path(question), :method => :delete, :title => 'delete' %>]
	<% end %>
<% end %>