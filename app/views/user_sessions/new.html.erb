<h1>be in</h1>

<% form_for @user_session, :url => user_session_path do |f| %>
  <%= f.error_messages %>
  <%= f.label :username, 'yo name' %><br />
  <%= f.text_field :username %><br />
  <br />
  <%= f.label :password, 'yo word' %><br />
  <%= f.password_field :password %><br />
  <br />
  <%= f.check_box :remember_me %><%= f.label :remember_me, 'Oh, you remembered me?!'%><br />
  <br />
  <%= f.submit "go" %>
<% end %>
<%= link_to "I'm new", new_user_path %><br>
<%= link_to 'I forgot.', new_password_reset_path%>