<% div_for website, :class => 'borderedbox' do %>
  <%= voting_booth website %>
  <%= link_to website.name, website %>
  <%= add_to_list website %>
  <%= tagger website %>
  <%= tag_display website %>
  <% if current_user.andand.admin? %>
	  [<%= link_to 'x', website_path(website), :method => :delete, :title => 'delete' %>]
	<% end %>
<% end %>
