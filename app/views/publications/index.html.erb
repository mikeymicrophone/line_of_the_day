<h1>Listing publications</h1>

<table>
  <tr>
    <th>Text</th>
    <th>Line</th>
    <th>Group</th>
    <th>User</th>
  </tr>

<% @publications.each do |publication| %>
  <tr>
    <td><%=h publication.text %></td>
    <td><%=h publication.line_id %></td>
    <td><%=h publication.group_id %></td>
    <td><%=h publication.user_id %></td>
    <td><%= link_to 'Show', publication %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New publication', new_publication_path %>